Ph = float(input())
Duritatea = float(input())
Cl = float(input())
Nitrit = float(input())
nitrat = float(input())

if Ph >= 8.5 or Ph <= 6.5:
    print(" este rau")
else:
    print(" este buna")
if Duritatea > 2 or Duritatea < 0:
    print(" nu este buna duritatea")
else:
    print("este buna duritatea")

if Cl >0.5 or Cl <0.3 :
    print("Nu se ajung ioni de clor")
else:
    print("clor este deajuns")

if Nitrit >0.5:
    print("nitrit este mult")
else:
    print("nitrit este deajuns")

if nitrat >10:
    print("nitrat este mult")
else:
    print("nitrat este deajuns")
